{
  "name": "medical_erp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn run relay && next dev -p 3003",
    "relay": "yarn run relay-compiler",
    "schema": "graphql fetch-schema -e dev",
    "build": "yarn run relay && next build",
    "start": "yarn run relay && next start",
    "lint": "next lint"
  },
  "relay": {
    "src": "./",
    "schema": "./schema.graphql",
    "language": "typescript",
    "artifactDirectory": "./__generated__",
    "eagerEsModules": false
  },
  "dependencies": {
    "@types/node": "20.4.9",
    "@types/react": "18.2.19",
    "@types/react-dom": "18.2.7",
    "antd": "^5.8.2",
    "eslint": "8.46.0",
    "eslint-config-next": "13.4.13",
    "graphql": "^16.8.0",
    "next": "13.4.13",
    "next-auth": "^4.23.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-relay": "^15.0.0",
    "recoil": "^0.7.7",
    "recoil-relay": "^0.1.0",
    "relay-runtime": "^15.0.0",
    "typescript": "5.1.6"
  },
  "devDependencies": {
    "graphql-cli": "^4.1.0",
    "relay-compiler": "^15.0.0",
    "relay-compiler-language-typescript": "^15.0.1",
    "babel-plugin-relay": "^15.0.0",
    "sass": "^1.65.1"
  }
}
